{
    "env": {
        "browser": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "semi": [2, "always"],
        "no-throw-literal": 0,
        "indent": ["error", "tab"],
        "no-tabs": 0
    }
}
