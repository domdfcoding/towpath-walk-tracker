{
  "compilerOptions": {
    "outDir": "./towpath_walk_tracker/static/js",
    "allowJs": true,
    "target": "es2015",
    "types": ["leaflet-geometryutil", "leaflet-polylinedecorator", "leaflet.awesome-markers"],
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "useUnknownInCatchVariables": true,
    "strict": true,
    "exactOptionalPropertyTypes": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "noImplicitThis": true,
    "noFallthroughCasesInSwitch": true,
    "allowUnreachableCode": false
  },
  "include": ["./src/zoom_state.ts"],
  "exclude": ["./src/core/*"],
  "noEmit": true,
  "noEmitOnError": false
}
